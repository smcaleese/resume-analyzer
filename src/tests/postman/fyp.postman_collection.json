					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Status Code', function() {\r",
							"    pm.response.to.have.status(200)\r",
							"})\r",
							"\r",
							"pm.test('Skills', function() {\r",
							"    var data = pm.response.json();\r",
							"    var skillsList = data.skills;\r",
							"    pm.expect(skillsList[0].name).to.equal('ReactJS')\r",
							"    pm.expect(skillsList[1].name).to.equal('Python')\r",
							"    pm.expect(skillsList[2].name).to.equal('Java')\r",
							"    pm.expect(skillsList[3].name).to.equal('JavaScript')\r",
							"})"
						],
